{
  "name": "reactexpress",
  "version": "1.0.0",
  "description": "Create a boilerplate for react-express",
  "main": "server.js",
  "scripts": {
    "client-install": "cd client && npm install",
    "start": "node index.js",
    "server": "nodemon server.js",
    "client": "npm start --prefix client",
    "dev": "concurrently \"npm start\" \"npm run client\""
